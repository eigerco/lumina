        "typescript": "^5.6.3"
    },
    "scripts": {
        "tsc": "tsc",
        "update-readme": "typedoc --plugin typedoc-plugin-markdown --out ./docs --readme ../README.md ../pkg/lumina_node_wasm.d.ts && concat-md  --decrease-title-levels --dir-name-as-title docs > README.md"
